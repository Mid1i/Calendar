{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src/components/BaseButton.vue"],
    "exclude": ["node_modules"],
    "includePattern": ".+\\.(ts|vue)$",
    "excludePattern": "(node_module|vendor)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": true,
      "outputSourcePath": true
    }
  }
}